<div id="surveyheader">
	<% if (exitText != null && exitText !== ''){ %>
		<button id="<%= exitID %>"><%= exitText %><br /></button>	
	<% } %>

	<% if (survey.showWhoFor) { %>
		<%= SURVEY_CLIENT.MESSAGES.PROBAND_PREFIX %> <u><%= survey.proband_name %></u>
	<% } %>
	<h1 id="survey_title"><%= survey.title %></h1>
	<div id="ajax-busy" class="hidden"></div>
	<h3 id="survey_subtitle"><%= survey.description %></h3>
</div>

